<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>Fly High</title>
  <meta content="" name="descriptison">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="assets/img/favicon.png" rel="icon">
  <link href="assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="assets/vendor/icofont/icofont.min.css" rel="stylesheet">
  <link href="assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="assets/vendor/venobox/venobox.css" rel="stylesheet">
  <link href="assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">

  <link href="styles/css/style.css" rel="stylesheet">


</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top ">
    <div class="container d-flex align-items-center">

      <h1 class="logo"><a href="/Airline_Profile" style="color:#d9232d;">Fly High</a></h1>
      <nav class="nav-menu d-none d-lg-block">

        <ul>
          <li><a href="/Airline_Profile">Airline Profile</a></li>          
          <li class="drop-down"><a href="#">Airline Flights</a>
            <ul>
              <li><a href="/Airline_Add_Flight">Add Flights</a></li>
              <li><a href="/Airline_Cancel_Flight">Cancel Flights</a></li>
              <li><a href="/Airline_Flight_Discount">Discount On Flights</a></li>          
            </ul>
          <li class><a href="/Airline_Flight_Scheduled">Current Flights</a></li>
          <li class = "active"><a href="/Airline_Flight_History">Flight History</a></li>   
        </ul>

      </nav><!-- .nav-menu -->
      <a role="button" href="/Logout" class="btn ml-auto" style="background-color:#d9232d;color:white">Logout</a>
    </div>
  </header><!-- End Header -->
   
  <!-- ======= Hero Section ======= -->
  <section id="hero">
    <div id="heroCarousel" class="carousel slide carousel-fade" data-ride="carousel">

      <ol class="carousel-indicators" id="hero-carousel-indicators"></ol>

      <div class="carousel-inner" role="listbox">

        <!-- Slide 1 -->
        <div class="carousel-item active" style="background-image: url(assets/img/Airline1.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown"><span><%= N %></span></h2>
              <p class="animate__animated animate__fadeInUp">Look All Your Flights</p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Flight History</a>
            </div>
          </div>
        </div>

        <!-- Slide 2 -->
        <div class="carousel-item" style="background-image: url(assets/img/Airline2.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown"><span><%= N %></span></h2>
              <p class="animate__animated animate__fadeInUp">Look All Your Flights</p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Flight History</a>
            </div>
          </div>
        </div>

        <!-- Slide 3 -->
        <div class="carousel-item" style="background-image: url(assets/img/Airline3.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown"><span><%= N %></span></h2>
              <p class="animate__animated animate__fadeInUp">Look All Your Flights</p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Flight History</a>
            </div>
          </div>
        </div>

      </div>

      <a class="carousel-control-prev" href="#heroCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon icofont-simple-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>

      <a class="carousel-control-next" href="#heroCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon icofont-simple-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>

    </div>
  </section>
  <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <div class="d-flex justify-content-between align-items-center">
          <h2 style="color:#d9232d;"><%= N %> Profile</h2>
          <ol>
            <li><a href="index.html">Home</a></li>
            <li><%= N %> Profile</li>
          </ol>
        </div>

      </div>
    </section>


  <main id="main">


    <section id="portfolio" class="portfolio">
      <div class ="row">
        <div class="col-sm-12">
          <table class="table">
            <thead class="table-head" style = "background: #d9232d;color:white">
              <tr>
                <th scope="col">Airline Name</th>
                <th scope="col">Flight Number</th>
                <th scope="col">Source</th>
                <th scope="col">Destination</th>
                <th scope="col">Departure Time</th>
                <th scope="col">Arrival Time</th>
                <th scope="col">Seats In Business Class</th>
                <th scope="col">Business Class Price</th>
                <th scope="col">Seats In Economic Class</th>
                <th scope="col">Economic Class Flight</th>                
              </tr>
            </thead>
            <tbody>
              <% for(i=0;i<F.length;i++){ %>
              <tr>
                <td> <%= F[i].Airline_Name %> </td> 
                <td> <%= F[i].Flight_Number %> </td>
                <td> <%= F[i].Flight_Source %> </td>
                <td> <%= F[i].Flight_Destination %> </td>
                <td> <%= F[i].DEPARTURE_TIME %> </td>
                <td> <%= F[i].ARRIVAL_TIME %> </td>
                <td> <%= F[i].S1 %> </td>
                <td> <%= F[i].P1 %> </td>
                <td> <%= F[i].S2 %> </td>                
                <td> <%= F[i].P2 %> </td>
              </tr> 
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </section>


  </main><!-- End #main -->

 <!-- ======= Footer ======= -->
