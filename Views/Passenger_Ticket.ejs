<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top ">
        <div class="container d-flex align-items-center">
            <h1 class="logo"><a href="#">FLY HIGH</a></h1>
        </div>
    </header>
    <!-- End Header -->

    <!-- Main Content-->
    <main id="main">
        <!-- Passenger Ticket Section -->
        <section id="passengerTicket" class="passengerTicket">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <h2 class="passengerTicketHeading">Ticket</h2>
                    </div>
                    <div class="col-8 offset-2">
                        <div class="card">
                            <h3 class="card-header text-white text-center"><%=A%></h3>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-4 offset-1">
                                        <h4 class="text-center"><%=B%></h4>
                                    </div>
                                    <div class="col-2">
                                        <img src="assets/img/Airline/planeVector.JPEG" width="80" height="80">
                                        <h5 id="ticketFlightNumber"><%=C%></h5>
                                    </div>
                                    <div class="col-4">
                                        <h4 class="text-center"><%=D%></h4>
                                    </div>
                                </div>
                                <div id="passengerNameHeading" class="row">
                                    <div class="col-6">
                                        <span>Passenger Name</span>
                                        <h5><%=E%></h5>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <span>Class</span>
                                        <h5><%=F%></h5>
                                    </div>
                                    <div class="col-6">
                                        <span>Flight Number</span>
                                        <h5><%=G%></h5>
                                    </div>
                                </div>
                                <div id="flightTimes" class="row">
                                    <div class="col-6">
                                        <span>Departure Time</span>
                                        <h5><%=H%></h5>
                                    </div>
                                    <div class="col-6">
                                        <span>Arrival Time</span>
                                        <h5><%=I%></h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End Passenger Ticket Section -->
    </main>
    <!-- End Main Content -->

    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="footer-top">
            <div class="container">
            <div class="row">

                <div class="col-lg-3 col-md-6">
                <div class="footer-info">
                    <h3>FLY HIGH</h3>
                    <p>
                    FAST NUCES <br>
                    Karachi 75950, Pakistan<br><br>
                    <strong>Phone:</strong> +1 2345 2345 67<br>
                    <strong>Email:</strong> K123456@nu.edu.pk<br>
                    </p>
                </div>
                </div>

                <div class="col-lg-4 col-md-6 footer-newsletter">
                <h4>Motto</h4>
                <p>We offer you best flight booking experience ever with top airlines available right now.</p>
                </div>
            </div>
            </div>
        </div>

        <div class="container">
            <div class="copyright">
            &copy; Copyright <strong><span>Fly High</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
            Designed by <a href="https://bootstrapmade.com/">Saif-Aly-Zain</a>
            </div>
        </div>
    </footer>
    <!-- End Footer -->

    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>

