<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top ">
    <div class="container d-flex align-items-center">

      <h1 class="logo"><a href="/Airline_Profile" style="color:#d9232d;">Fly High</a></h1>
      <nav class="nav-menu d-none d-lg-block">

        <ul>
          <li class = "active"><a href="/Airline_Profile">Airline Profile</a></li>          
          <li class="drop-down"><a href="#">Airline Flights</a>
            <ul>
              <li><a href="/Airline_Add_Flight">Add Flights</a></li>
              <li><a href="/Airline_Cancel_Flight">Cancel Flights</a></li>
              <li><a href="/Airline_Flight_Discount">Discount On Flights</a></li>          
            </ul>
          <li class><a href="/Airline_Flight_Scheduled">Current Flights</a></li>
          <li class><a href="/Airline_Flight_History">Flight History</a></li>   
        </ul>

      </nav><!-- .nav-menu -->
      <a role="button" href="/Logout" class="btn ml-auto" style="background-color:#d9232d;color:white">Logout</a>
    </div>
  </header><!-- End Header -->
   
  <!-- ======= Hero Section ======= -->
  <section id="hero">
    <div id="heroCarousel" class="carousel slide carousel-fade" data-ride="carousel">

      <ol class="carousel-indicators" id="hero-carousel-indicators"></ol>

      <div class="carousel-inner" role="listbox">

        <!-- Slide 1 -->
        <div class="carousel-item active" style="background-image: url(assets/img/Airline1.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown">Lets Fly <span><%= F[0].Airline_Name %></span></h2>
              <p class="animate__animated animate__fadeInUp">Fly High Welcomes <%= F[0].Airline_Name %> For Believing In Us </p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Profile</a>
            </div>
          </div>
        </div>

        <!-- Slide 2 -->
        <div class="carousel-item" style="background-image: url(assets/img/Airline2.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown">Lets Fly <span><%= F[0].Airline_Name %></span></h2>
              <p class="animate__animated animate__fadeInUp">Fly High Welcomes <%= F[0].Airline_Name %> For Believing In Us </p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Profile</a>
            </div>
          </div>
        </div>

        <!-- Slide 3 -->
        <div class="carousel-item" style="background-image: url(assets/img/Airline3.jpg)">
          <div class="carousel-container">
            <div class="container">
              <h2 class="animate__animated animate__fadeInDown">Lets Fly <span><%= F[0].Airline_Name %></span></h2>
              <p class="animate__animated animate__fadeInUp">Fly High Welcomes <%= F[0].Airline_Name %> For Believing In Us </p>
              <a href="#breadcrumbs" class="btn-get-started animate__animated animate__fadeInUp scrollto">View Profile</a>
            </div>
          </div>
        </div>

      </div>

      <a class="carousel-control-prev" href="#heroCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon icofont-simple-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>

      <a class="carousel-control-next" href="#heroCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon icofont-simple-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>

    </div>
  </section>
  <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <div class="d-flex justify-content-between align-items-center">
          <h2 style="color:#d9232d;"><%= F[0].Airline_Name %> Profile</h2>
          <ol>
            <li><a href="index.html">Home</a></li>
            <li><%= F[0].Airline_Name %> Profile</li>
          </ol>
        </div>

      </div>
  </section>


  <main id="main">
 
    <% if (I == 1) { %>
      <div class="alert alert-success">
      <strong>          Success! </strong>Password Modified.
    </div>
    <% } %>

    <% if (I == 0) { %>
      <div class="alert alert-warning">
      <strong>Try Again! </strong>. Please Enter Your Correct Password.
    </div>
    <% } %>


    <section id="portfolio" class="portfolio">
      <div class ="row">
        <div class = "col-sm-1"></div>  
        <div class = "col-sm-4">
          <div class="card" style="width: 37rem;">
            <img class="card-img-top" src="assets/img/flyHigh.jpeg" alt="Card image cap">
            <div class="card-body">
              <div class="row">
                <div class="col-md-6 mb-3">
                  <label for="name" style="color:#d9232d;">Airline Name</label>
                  <div><label> <%= F[0].Airline_Name %> </label></div>
                </div>
                <div class="col-md-6 mb-3">
                  <label for="Email" style="color:#d9232d;">Airline Email</label>
                  <div><label><%= F[0].Airline_ID %> </label></div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6 mb-3">
                  <label for="AirlineOwner" style="color:#d9232d;" >Airline Owner</label>
                  <div><label><%= F[0].Airline_Owner %> </label></div>
                </div>
                <div class="col-md-6 mb-3">
                  <label for="AirlineOrigin" style="color:#d9232d;">Airline Origin</label>
                <div><label><%= F[0].Airline_Origin %></label></div>
                </div>                      
              </div>  
              <div class="row">
                <div class="col-md-6 mb-3">
                  <label for="AirlineAbbrevation" style="color:#d9232d;">Airline Abbrevation</label>
                  <div><label><%= F[0].Airline_Abbreviation %></label></div>                       
                </div>                       
                <div class="col-md-6 mb-3">
                  <label for ="AirlineEstablished" style="color:#d9232d;">Established</label>
                <div><label><%= F[0].Established_Year %></label></div> 
                </div>
              </div>                                       
            </div>                               
          </div>
        </div>
        <div class = "col-sm-2"></div>
        <div class = "col-sm-4 " style = "padding-top: 100px";>
          <div class="card">
            <img class="card-img-top" src="assets/img/flyHigh2.jpeg" alt="Card image cap">
            <div class="card-body">
              <form action = "/Change" method = "POST">
                <div class="form-row">
                  <div class="col-md-6 mb-3">
                    <input type="password" class="form-control" name = "OldPassword" id="OldPassword" placeholder = "Old Password" required>
                  </div>
                  <div class="col-md-6 mb-3">
                    <input type="password" class="form-control" name = "NewPassword" id="NewPassword" placeholder = "New Password"  required>
                  </div>                  
                </div>
                  <button class="btn offset-4" type="submit"style="background-color:#d9232d;color:white">Update</button>                                   
              </form> 
            </div>
          </div>               
        </div>
        <div class = "col-sm-1"></div>      
      </div>
    </section>


  </main><!-- End #main -->

